pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    stage('Build') {
            steps {
                echo 'Building'
            }
        }
    stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    stage('Test') {
            steps {
                echo 'Testing'
            }
        }
    stage('Release') {
            steps {
                echo 'Releasing'
            }
        }   
    stage('Execute') {
            steps {
                echo 'Executing'
            }
        }
     stage('Check') {
            steps {
                echo 'checking'
            }    
        }
    }
}